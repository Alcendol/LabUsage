<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:vm="using:LabUsage.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="LabUsage.SecretView"
		x:DataType="vm:SecretViewModel"
        Title="SecretView">
	<StackPanel>
        <TextBlock Text="Super Secret View" FontSize="18" FontWeight="Bold" Margin="5 20 5 10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
		<TextBox Text="{Binding Token}" IsReadOnly="True"
				 HorizontalAlignment="Center"
				 TextWrapping="Wrap"
				 Margin="12 5 12 12"
				 MaxWidth="500" />
	</StackPanel>
</Window>
